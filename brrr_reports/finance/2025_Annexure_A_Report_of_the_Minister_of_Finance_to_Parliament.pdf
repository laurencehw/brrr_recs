        <script language="javascript" type="text/javascript">
        function showMenu(x,totalMenus)
        {
            var str = 'SubMenu' + x;
            var mId = document.getElementById(str).style;
            
        	if (mId.display == 'block' )
		        mId.display = 'none';
	        else
	        {	
		        for (var i = 1; i < totalMenus+1; i++)
		        {
                    var strC = 'SubMenu' + i;
                    var mIdC = document.getElementById(strC).style;
			        mIdC.display = 'none';
		        }
		        mId.display = 'block';
	        }	
        }
        
        </script>

